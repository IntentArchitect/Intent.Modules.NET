{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DaprSidekick": {
    "Sidecar": {
      "AppId": "publish-clean-arch-dapr-test-application",
      "ComponentsDirectory": "../dapr/components",
      "ConfigFile": "../dapr/config.yaml"
    }
  },
  "Security.Bearer": {
    "Authority": "https://localhost:{sts_port}",
    "Audience": "api"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}