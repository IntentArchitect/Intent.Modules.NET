<?xml version="1.0" encoding="utf-8"?>
<package>
  <id>Intent.OpenTelemetry</id>
  <version>2.2.2</version>
  <supportedClientVersions>[4.3.0-a, 5.0.0-a)</supportedClientVersions>
  <summary>Introduces an open framework for collecting and exporting telemetry data.</summary>
  <description>Introduces an open framework for collecting and exporting telemetry data.</description>
  <authors>Intent Architect</authors>
  <iconUrl>data:image/png;base64,</iconUrl>
  <releaseNotes>release-notes.md</releaseNotes>
  <tags>csharp dotnet telemetry apm application insight appinsights monitoring</tags>
  <templates>
    <template id="Intent.OpenTelemetry.OpenTelemetryConfiguration" externalReference="9ee62e7e-be4a-45e9-925a-92f7653bde12">
      <config>
        <add key="ClassName" description="Class name formula override (e.g. '${Model.Name}')" />
        <add key="Namespace" description="Class namespace formula override (e.g. '${Project.Name}'" />
      </config>
      <role>Distribution.OpenTelemetry</role>
      <location>Configuration</location>
    </template>
  </templates>
  <decorators></decorators>
  <factoryExtensions>
    <factoryExtension id="Intent.OpenTelemetry.ProgramConfigurationExtension" externalReference="741f5903-5068-49c4-aeeb-31480f0e14a6" />
  </factoryExtensions>
  <moduleSettings>
    <group id="ea731c11-8d05-46e0-8040-7ee95a78e27e" title="Open Telemetry" externalReference="ea731c11-8d05-46e0-8040-7ee95a78e27e">
      <settings>
        <setting id="ccbdc18f-b1c3-4f4b-a1a7-fa29c626ce0e" title="Export" type="select">
          <hint>Where to export the Telemetry data to.</hint>
          <defaultValue>console</defaultValue>
          <options>
            <option value="console" description="Console" />
            <option value="open-telemetry-protocol" description="OpenTelemetry Protocol" />
            <option value="azure-application-insights" description="Azure Application Insights" />
            <option value="azure-monitor-opentelemetry-distro" description="Azure Monitor OpenTelemetry Distro" />
          </options>
        </setting>
        <setting id="93dbe976-bebb-47e9-a59e-0367271f3dbb" title="Capture Traces" type="checkbox">
          <hint>Include traces in telemetry export.</hint>
          <defaultValue>true</defaultValue>
          <isActiveFunction>const Export = "ccbdc18f-b1c3-4f4b-a1a7-fa29c626ce0e";
const AzureMonitorOpenTelemetryDistro = "azure-monitor-opentelemetry-distro";

return getProperty(Export).value != AzureMonitorOpenTelemetryDistro;</isActiveFunction>
        </setting>
        <setting id="b919cb80-15dc-4f1e-820c-faaddf4c0063" title="Capture Metrics" type="checkbox">
          <hint>Include metrics in telemetry export.</hint>
          <defaultValue>false</defaultValue>
          <isActiveFunction>const Export = "ccbdc18f-b1c3-4f4b-a1a7-fa29c626ce0e";
const AzureMonitorOpenTelemetryDistro = "azure-monitor-opentelemetry-distro";

return getProperty(Export).value != AzureMonitorOpenTelemetryDistro;</isActiveFunction>
        </setting>
        <setting id="7cbb4a05-7dac-4b06-b2da-635ab82dd125" title="Capture Logs" type="checkbox">
          <hint>Include logs (i.e. Serilog) in telemetry export.</hint>
          <defaultValue>true</defaultValue>
          <isActiveFunction>const Export = "ccbdc18f-b1c3-4f4b-a1a7-fa29c626ce0e";
const AzureMonitorOpenTelemetryDistro = "azure-monitor-opentelemetry-distro";

return getProperty(Export).value != AzureMonitorOpenTelemetryDistro;</isActiveFunction>
        </setting>
        <setting id="11352624-4a31-4ec9-8768-0dfef81cf3b1" title="ASP.NET Core Instrumentation" type="checkbox">
          <hint>Include ASP.NET Core instrumentation (Trace + Metrics).</hint>
          <defaultValue>true</defaultValue>
          <isActiveFunction>const CaptureTraces = "93dbe976-bebb-47e9-a59e-0367271f3dbb";
const CaptureMetrics = "b919cb80-15dc-4f1e-820c-faaddf4c0063";
const Export = "ccbdc18f-b1c3-4f4b-a1a7-fa29c626ce0e";
const AzureMonitorOpenTelemetryDistro = "azure-monitor-opentelemetry-distro";

return getProperty(Export).value != AzureMonitorOpenTelemetryDistro &amp;&amp; 
    (getProperty(CaptureTraces).value == "true" || getProperty(CaptureMetrics).value == "true");</isActiveFunction>
        </setting>
        <setting id="d41bdd38-bfc0-4c96-8cb2-0bd4b6f01f01" title="HTTP Instrumentation" type="checkbox">
          <hint>Include HTTP instrumentation (Trace + Metrics).</hint>
          <defaultValue>false</defaultValue>
          <isActiveFunction>const CaptureTraces = "93dbe976-bebb-47e9-a59e-0367271f3dbb";
const CaptureMetrics = "b919cb80-15dc-4f1e-820c-faaddf4c0063";
const Export = "ccbdc18f-b1c3-4f4b-a1a7-fa29c626ce0e";
const AzureMonitorOpenTelemetryDistro = "azure-monitor-opentelemetry-distro";

return getProperty(Export).value != AzureMonitorOpenTelemetryDistro &amp;&amp;
    (getProperty(CaptureTraces).value == "true" || getProperty(CaptureMetrics).value == "true");</isActiveFunction>
        </setting>
        <setting id="14d18546-83cd-46b6-8d0e-c9d098ece8e0" title="SQL Instrumentation" type="checkbox">
          <hint>Include SQL instrumentation (Trace).</hint>
          <defaultValue>false</defaultValue>
          <isActiveFunction>const CaptureTraces = "93dbe976-bebb-47e9-a59e-0367271f3dbb";
const Export = "ccbdc18f-b1c3-4f4b-a1a7-fa29c626ce0e";
const AzureMonitorOpenTelemetryDistro = "azure-monitor-opentelemetry-distro";

return getProperty(Export).value != AzureMonitorOpenTelemetryDistro &amp;&amp;
    getProperty(CaptureTraces).value == "true";</isActiveFunction>
        </setting>
        <setting id="dd698842-3b42-4ddd-8a90-cb67a12ed375" title="Process Instrumentation" type="checkbox">
          <hint>Include Process instrumentation (Metrics).</hint>
          <defaultValue>false</defaultValue>
          <isActiveFunction>const CaptureMetrics = "b919cb80-15dc-4f1e-820c-faaddf4c0063";
const Export = "ccbdc18f-b1c3-4f4b-a1a7-fa29c626ce0e";
const AzureMonitorOpenTelemetryDistro = "azure-monitor-opentelemetry-distro";

return getProperty(Export).value != AzureMonitorOpenTelemetryDistro &amp;&amp;
    getProperty(CaptureMetrics).value == "true";</isActiveFunction>
        </setting>
        <setting id="93367ae8-466f-4e0e-a84c-19eae4b80daa" title=".NET Runtime Instrumentation" type="checkbox">
          <hint>Include .NET Runtime instrumentation (Metrics).</hint>
          <defaultValue>false</defaultValue>
          <isActiveFunction>const CaptureMetrics = "b919cb80-15dc-4f1e-820c-faaddf4c0063";
const Export = "ccbdc18f-b1c3-4f4b-a1a7-fa29c626ce0e";
const AzureMonitorOpenTelemetryDistro = "azure-monitor-opentelemetry-distro";

return getProperty(Export).value != AzureMonitorOpenTelemetryDistro &amp;&amp;
    getProperty(CaptureMetrics).value == "true";</isActiveFunction>
        </setting>
      </settings>
    </group>
  </moduleSettings>
  <interoperability>
    <!-- Minimum Compatible Versions -->
    <detect id="Intent.AspNetCore">
      <install>
        <package id="Intent.AspNetCore" version="6.0.2" />
      </install>
    </detect>
    <!-- /Minimum Compatible Versions -->
    <!-- Remove after 1 January 2025 -->
    <detect id="Intent.Eventing.MassTransit">
      <install>
          <package id="Intent.Eventing.MassTransit" version="6.2.6" />
      </install>
    </detect>
    <!-- Remove after 1 January 2025 -->
  </interoperability>
  <dependencies>
    <dependency id="Intent.Common" version="3.5.0" />
    <dependency id="Intent.Common.CSharp" version="3.8.0" />
    <dependency id="Intent.Common.Types" version="3.3.9" />
    <dependency id="Intent.OutputManager.RoslynWeaver" version="4.1.1" />
  </dependencies>
  <files>
    <file src="$outDir$/$id$.dll" />
    <file src="$outDir$/$id$.pdb" />
  </files>
  <moduleSettingsExtensions />
  <metadata />
  <tags />
</package>