        <setting id="3e3d24f8-ad29-44d6-b7e5-e76a5af2a7fa" title="Render Mode" type="select">
          <hint>Determines how the page content is rendered at runtime.</hint>
          <defaultValue>interactive-server</defaultValue>
          <options>
            <option value="interactive-server" description="Interactive Server" />
            <option value="interactive-web-assembly" description="Interactive Web Assembly" />
            <option value="interactive-auto" description="Interactive Auto" />
          </options>
        </setting>
        <setting id="bbddad39-3601-4244-bf8d-0da805ee6376" title="Include Sample Pages" type="switch">
          <defaultValue>false</defaultValue>
        </setting>
        <setting id="d851b4d1-b230-461f-9873-80d6857fa175" title="Prerendering" type="switch">
          <hint>Enable prerendering for Interactive Server mode.</hint>
          <defaultValue>false</defaultValue>
          <isActiveFunction>return getProperty("Render Mode").value != "interactive-web-assembly";</isActiveFunction>
        </setting>
      </settings>
    </group>
