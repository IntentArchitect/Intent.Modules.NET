<stereotypeDefinition id="fd52e71a-3810-4ae7-ae40-4e1514903d25" version="3.1.0">
  <comment>Configures launch setting information for the application</comment>
  <name>Launch Settings</name>
  <targetMode>filter-function</targetMode>
  <targetTypes>
    <type id="8e9e6693-2888-4f48-a0d6-0f163baab740">C# Project (.NET)</type>
  </targetTypes>
  <targetReferenceTypes />
  <targetFilterFunction>function hasLaunchSettings(element) {
    if(!element.hasStereotype(".NET Settings")) {
        return false;
    }
    let netSettings = element.getStereotype(".NET Settings");

    if(!netSettings.hasProperty("SDK")) {
        return false;
    }
    let sdk = netSettings.getProperty("SDK");

    let launchSettings = (netSettings.hasProperty("Generate LaunchSettings File") 
        &amp;&amp; netSettings.getProperty("Generate LaunchSettings File").value == true);

    return sdk.value == "Microsoft.NET.Sdk.Web" || sdk.value == "Microsoft.NET.Sdk.BlazorWebAssembly" ||
        sdk.value == "Microsoft.NET.Sdk.Worker" || (sdk.value == "Microsoft.NET.Sdk" &amp;&amp; launchSettings);
}

console.warn(element.getName());

// is it ASP.NET Core Web Application
let isWebApp = element.specializationId == "FFD54A85-9362-48AC-B646-C93AB9AC63D2";
// is it C# Project (.NET)
let isCSharpProject = element.specializationId == "8e9e6693-2888-4f48-a0d6-0f163baab740";

return isWebApp || (isCSharpProject &amp;&amp; hasLaunchSettings(element));</targetFilterFunction>
