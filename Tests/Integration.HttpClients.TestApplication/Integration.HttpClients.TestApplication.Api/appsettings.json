{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HttpClients": {
    "InvoiceProxy": {
      "Uri": "https://localhost:{app_port}/",
      "IdentityClientKey": "default",
      "Timeout": "00:01:00"
    },
    "MultiVersionServiceProxy": {
      "Uri": "https://localhost:{app_port}/",
      "IdentityClientKey": "default",
      "Timeout": "00:01:00"
    },
    "VersionOneServiceProxy": {
      "Uri": "https://localhost:{app_port}/",
      "IdentityClientKey": "default",
      "Timeout": "00:01:00"
    }
  },
  "IdentityClients": {
    "default": {
      "Address": "https://localhost:{sts_port}/connect/token",
      "ClientId": "clientId",
      "ClientSecret": "secret",
      "Scope": "api"
    }
  }
}