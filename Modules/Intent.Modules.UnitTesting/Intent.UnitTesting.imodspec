      <settings>
        <setting id="381ddc17-ca81-4f28-8636-5a4b547ff6d4" title="Unit Test Generation Mode" type="select">
          <hint>Controls whether unit test stubs are generated for all services or only those explicitly selected.</hint>
          <defaultValue>explicit</defaultValue>
          <options>
            <option value="all" description="Generate for all Commands, Queries and Service Operations" />
            <option value="explicit" description="Generate only when explicitly marked with Unit Test stereotype" />
          </options>
        </setting>
        <setting id="115c28bc-a4c8-4b30-bd00-2e320fee77dc" title="Mock Framework" type="select">
          <defaultValue>moq</defaultValue>
          <options>
            <option value="moq" description="Moq" />
