{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Local-Setting": "Yes, a local setting",
  "KeyVault": {
    "Enabled": false,
    "Endpoint": "https://VAULT-NAME-HERE.vault.azure.net/",
    "ClientId": "",
    "Secret": "",
    "TenantId": ""
  },
  "ApplicationInsights": {
    "ConnectionString": "Insert Application Insights Connection String Here"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] {Category:0} - {Message}{NewLine:1}{Exception}",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ]
  }
}