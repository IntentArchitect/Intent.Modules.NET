  <releaseNotes>release-notes.md</releaseNotes>
  <templates>
    <template id="Intent.AspNetCore.ODataQuery.ODataQuerySwaggerFilter" externalReference="794c2498-93df-46ce-ae0d-f10794f995e8">
      <config>
        <add key="ClassName" description="Class name formula override (e.g. '${Model.Name}')" />
        <add key="Namespace" description="Class namespace formula override (e.g. '${Project.Name}'" />
      </config>
      <role>Startup</role>
      <location>Filters</location>
    </template>
  </templates>
  <decorators></decorators>
  <factoryExtensions>
    <factoryExtension id="Intent.AspNetCore.ODataQuery.CosmosRepositoryExtension" externalReference="dd400bf9-1717-4804-9a97-4c548f6865c3" />
    <factoryExtension id="Intent.AspNetCore.ODataQuery.EFRepositoryExtension" externalReference="b5235e94-ce0d-4278-a5c5-0ee52c1c6bcf" />
    <factoryExtension id="Intent.AspNetCore.ODataQuery.QueryExtension" externalReference="1ee783d5-7aca-40cb-a37b-65e018985876" />
    <factoryExtension id="Intent.AspNetCore.ODataQuery.StartupExtension" externalReference="79f071dc-8f83-4823-983e-118dcedc3d7d" />
    <factoryExtension id="Intent.AspNetCore.ODataQuery.SwaggerConfigExtension" externalReference="d03b3262-faad-40cc-9961-40b9b913596b" />
  </factoryExtensions>
  <moduleSettings>
    <group id="f9a82d4b-bb1b-4fa5-9586-36485364e6fc" title="OData Query Settings" externalReference="f9a82d4b-bb1b-4fa5-9586-36485364e6fc">
      <settings>
        <setting id="f07b3386-b22c-48b7-b39a-3b113d0984e3" title="Allow Filter Option" type="switch">
          <isRequired>false</isRequired>
          <hint>Enable $filter query option. This allows  expression based filtering of data.</hint>
          <defaultValue>true</defaultValue>
        </setting>
        <setting id="35ddc434-96db-4d85-b8e8-8b724e251897" title="Allow OrderBy Option" type="switch">
          <isRequired>false</isRequired>
          <hint>Enable $orderby query option. This allows  expression based ordering of the data.</hint>
          <defaultValue>true</defaultValue>
        </setting>
        <setting id="e1ef66ba-3e3c-47c5-8a25-55bd827fcd3b" title="Allow Expand Option" type="switch">
          <isRequired>false</isRequired>
          <hint>Enable $expand query options. This option specifies the related data to be included in line with the query.</hint>
          <defaultValue>true</defaultValue>
        </setting>
        <setting id="73866e5a-1fba-4637-af2b-9c3cfc1786cd" title="Allow Select Option" type="switch">
          <isRequired>false</isRequired>
          <hint>Enable $select query options. This option allows you to change the nature of the return type.</hint>
          <defaultValue>false</defaultValue>
        </setting>
        <setting id="8bafbb9e-e756-4e07-8ff3-da57e4b3c190" title="Max Top" type="number">
          <isRequired>false</isRequired>
          <hint>Sets the maximum value of $top that a client can request</hint>
          <defaultValue>200</defaultValue>
        </setting>
      </settings>
    </group>
  </moduleSettings>
