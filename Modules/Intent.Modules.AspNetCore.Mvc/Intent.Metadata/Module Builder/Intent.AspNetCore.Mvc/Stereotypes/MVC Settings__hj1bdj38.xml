<stereotypeDefinition id="647586e0-09df-444a-9dc4-f13637b0c3ac" version="3.1.0">
  <comment>Configures settings related to MVC behavior for the operation.</comment>
  <name>MVC Settings</name>
  <targetMode>of-type</targetMode>
  <targetTypes>
    <type id="e030c97a-e066-40a7-8188-808c275df3cb">Operation</type>
  </targetTypes>
  <targetReferenceTypes />
  <applyMode>manually</applyMode>
  <icon type="UrlImagePath" source="data:image/svg+xml;base64," />
  <displayIcon>false</displayIcon>
  <displayIconFunction>return false;</displayIconFunction>
  <displayFunction>return `${properties["Verb"].value || "UNSPECIFIED"}("${properties["Route"].value}")`;</displayFunction>
  <displayAsAnnotation>true</displayAsAnnotation>
  <displayAsText>false</displayAsText>
  <autoAdd>false</autoAdd>
  <allowMultipleApplies>false</allowMultipleApplies>
  <parentFolderId>216bb401-5771-4803-a985-06f7c57a5e3d</parentFolderId>
