      <controlType>text</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder>(optional)</placeholder>
      <valueOptions />
      <lookupTypes />
      <isActiveFunction>let securityElementsDefined = lookupTypesOf("Security Configuration")[0]?.getChildren()?.length &gt; 0;
