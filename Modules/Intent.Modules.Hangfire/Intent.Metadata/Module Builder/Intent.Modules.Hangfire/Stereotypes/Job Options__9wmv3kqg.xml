  <applyMode>manually</applyMode>
  <icon type="FontAwesome" source="briefcase" />
  <displayIcon>false</displayIcon>
  <displayIconFunction>return false;</displayIconFunction>
  <displayFunction />
  <displayAsAnnotation>false</displayAsAnnotation>
