  <properties>
    <property id="ae0dda3e-f047-48ec-8779-3305e4ec9254">
      <name>Verb</name>
      <type>string</type>
      <controlType>select</controlType>
      <optionsSource>options</optionsSource>
      <defaultValue>GET</defaultValue>
      <valueOptions>
        <option>DELETE</option>
        <option>GET</option>
        <option>PATCH</option>
        <option>POST</option>
        <option>PUT</option>
      </valueOptions>
    </property>
    <property id="55641304-6fd1-451e-95f9-bbbc3f9e16f4">
      <name>Route</name>
      <type>string</type>
      <controlType>text</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder>(default)</placeholder>
    </property>
    <property id="ffbf2921-a1f3-4c23-a584-9055f7cd0402">
      <hint>For the `Name` property of the `Http&lt;Verb&gt;` attribute.</hint>
      <name>Name</name>
      <type>string</type>
      <controlType>text</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder>(optional)</placeholder>
    </property>
    <property id="8d37a88a-b540-402e-bca2-c4d8ff11c905">
      <name>Return Type</name>
      <type>string</type>
      <controlType>select</controlType>
      <optionsSource>options</optionsSource>
      <defaultValue>View</defaultValue>
      <valueOptions>
        <option>(Operation Return Type)</option>
        <option>RedirectToAction</option>
        <option>View</option>
        <option>Ok</option>
      </valueOptions>
      <isRequiredFunction>return true;</isRequiredFunction>
    </property>
    <property id="88f938a7-7ac3-473c-b489-a62184ae1409">
      <name>View Name</name>
      <type>string</type>
      <controlType>text</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder>(default)</placeholder>
      <isActiveFunction>return properties["Return Type"].value == "View";</isActiveFunction>
    </property>
    <property id="54484630-53f0-415c-a338-ef20bfcf0500">
      <name>RedirectTo Controller</name>
      <type>string</type>
      <controlType>select</controlType>
      <optionsSource>lookup-element</optionsSource>
      <placeholder>(same controller)</placeholder>
      <lookupTypes>
        <type>b16578a5-27b1-4047-a8df-f0b783d706bd</type>
      </lookupTypes>
      <isActiveFunction>return properties["Return Type"].value == "RedirectToAction";</isActiveFunction>
    </property>
    <property id="1adef718-4dff-47ce-91b1-ec729a1f707a">
      <name>RedirectTo Action</name>
      <type>string</type>
      <controlType>select</controlType>
      <optionsSource>lookup-children</optionsSource>
      <lookupTypes>
        <type>e042bb67-a1df-480c-9935-b26210f78591</type>
        <type>e030c97a-e066-40a7-8188-808c275df3cb</type>
      </lookupTypes>
      <lookupInternalTargetPropertyId>return properties["RedirectTo Controller"].value != null
    ? properties["RedirectTo Controller"].value
    : element.getParent().id;</lookupInternalTargetPropertyId>
      <isActiveFunction>return properties["Return Type"].value == "RedirectToAction";</isActiveFunction>
      <isRequiredFunction>return true;</isRequiredFunction>
    </property>
    <property id="6e2a9abd-9e8a-47c8-ba3d-ec2951b3bbb9">
      <name>RedirectTo Route Values</name>
      <type>string</type>
      <controlType>text</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder>(optional)</placeholder>
      <isActiveFunction>return properties["Return Type"].value == "RedirectToAction";</isActiveFunction>
    </property>
    <property id="4a40f0aa-b871-48c3-ae6b-73db5c448e42">
      <name>RedirectTo Fragment</name>
      <type>string</type>
      <controlType>text</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder>(optional)</placeholder>
      <isActiveFunction>return properties["Return Type"].value == "RedirectToAction";</isActiveFunction>
    </property>
  </properties>
