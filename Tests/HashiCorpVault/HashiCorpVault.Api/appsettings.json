{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] {Category:0} - {Message}{NewLine:1}{Exception}",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ]
  },
  "HashiCorpVault": {
    "Enabled": true,
    "Vaults": [
      {
        "Name": "Test",
        "Url": "http://127.0.0.1:8200",
        "AuthMethod": {
          "Token": {
            "Token": "root_token"
          }
        },
        "Path": "creds",
        "MountPoint": "secret",
        "CacheTimeoutInSeconds": 5
      }
    ]
  }
}