    </None>
    <None Update="Templates\PagedListInterface\PagedListInterfaceTemplate.tt">
      <Generator>TextTemplatingFilePreprocessor</Generator>
      <LastGenOutput>PagedListInterfaceTemplate.cs</LastGenOutput>
