  <moduleSettings>
    <group id="41ae5a02-3eb2-42a6-ade2-322b3c1f1115" title="Multitenancy Settings" externalReference="41ae5a02-3eb2-42a6-ade2-322b3c1f1115">
      <settings>
        <setting id="e15fe0fb-be28-4cc5-8b85-37a07b7ca160" title="Strategy" type="select">
          <isRequired>true</isRequired>
          <hint>How the tenant is determined.</hint>
          <defaultValue>header</defaultValue>
          <options>
            <option value="header" description="Header Strategy" />
            <option value="claim" description="Claim Strategy" />
            <option value="host" description="Host Strategy" />
          </options>
        </setting>
        <setting id="e430d7bc-fac3-4528-93e8-e3f38bc0b925" title="Store" type="select">
          <isRequired>true</isRequired>
          <hint>Store for tenant information.</hint>
          <defaultValue>in-memory</defaultValue>
          <options>
            <option value="configuration" description="Configuration" />
            <option value="efcore" description="Entity Framework Core" />
            <option value="http-remote" description="HTTP Remote" />
            <option value="in-memory" description="In-Memory" />
          </options>
        </setting>
        <setting id="be7c671e-bbef-4d75-b42d-a6547de3ae82" title="Data Isolation" type="select">
          <isRequired>true</isRequired>
          <hint>Set the data isolation.</hint>
          <defaultValue>separate-database</defaultValue>
          <options>
            <option value="separate-database" description="Separate Databases" />
            <option value="shared-database" description="Shared Database" />
          </options>
        </setting>
        <setting id="d299a5bb-db30-474b-8bac-fa879151a1a3" title="Apply Multi Tenancy to Aggregates" type="select">
          <isRequired>false</isRequired>
          <hint>Automatically configure Aggregate entities for Multi Tenancy. Only applicable when "Data Isolation" is set to "Shared Database".</hint>
          <defaultValue>automatically</defaultValue>
          <options>
            <option value="automatically" description="Automatically" />
            <option value="manually" description="Manually" />
          </options>
        </setting>
      </settings>
    </group>
  </moduleSettings>
