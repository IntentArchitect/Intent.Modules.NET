{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Security.Bearer": {
    "Authority": "https://localhost:{sts_port}",
    "Audience": "api"
  },
  "QueueStorage": {
    "DefaultEndpoint": "UseDevelopmentStorage=true",
    "Queues": {
      "azurefunction-queuestorage-eventing-messages-productcreatedevent": {
        "Endpoint": null,
        "CreateQueue": true
      },
      "cleanarchitecture-queuestorage-eventing-messages-createproductcommand": {
        "Endpoint": null,
        "CreateQueue": true
      },
      "cleanarchitecture-queuestorage-eventing-messages-createstockforproductcommand": {
        "Endpoint": null,
        "CreateQueue": true
      }
    },
    "QueueTypeMap": {
      "CleanArchitecture.QueueStorage.Eventing.Messages.CreateProductCommand": "cleanarchitecture-queuestorage-eventing-messages-createproductcommand",
      "CleanArchitecture.QueueStorage.Eventing.Messages.CreateStockForProductCommand": "cleanarchitecture-queuestorage-eventing-messages-createstockforproductcommand",
      "AzureFunction.QueueStorage.Eventing.Messages.ProductCreatedEvent": "azurefunction-queuestorage-eventing-messages-productcreatedevent"
    }
  },
  "CqrsSettings": {
    "LogRequestPayload": true
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] {Category:0} - {Message}{NewLine:1}{Exception}",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ]
  }
}