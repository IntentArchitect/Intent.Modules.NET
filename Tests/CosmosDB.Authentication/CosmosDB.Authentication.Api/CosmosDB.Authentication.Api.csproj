    <ImplicitUsings>enable</ImplicitUsings>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\CosmosDB.Authentication.Application\CosmosDB.Authentication.Application.csproj" />
    <ProjectReference Include="..\CosmosDB.Authentication.Infrastructure\CosmosDB.Authentication.Infrastructure.csproj" />
