  <packageName>Intent.Modules.Hangfire</packageName>
  <properties>
    <property id="fba31c2d-a5f1-404e-8772-15f72b94e5b1">
      <name>Enabled</name>
      <type>string</type>
      <controlType>checkbox</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder />
      <defaultValue>true</defaultValue>
      <valueOptions />
      <lookupTypes />
    </property>
    <property id="1f4905ea-0c33-4dd6-b0e5-f7926f599f92">
      <hint>The type of Hangfire job</hint>
      <name>Job Type</name>
      <type>string</type>
      <controlType>select</controlType>
      <optionsSource>options</optionsSource>
      <placeholder />
      <defaultValue>Recurring</defaultValue>
      <valueOptions>
        <option>Recurring</option>
        <option>Delayed</option>
        <option>Fire and Forget</option>
      </valueOptions>
      <lookupTypes />
    </property>
    <property id="153f01ba-f229-4a2e-8f44-0ecd39924073">
      <hint>The cron schedule on which the job will execute</hint>
      <name>Cron Schedule</name>
      <type>string</type>
      <controlType>text</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder>(example: */10 * * * * ?)</placeholder>
      <valueOptions />
      <lookupTypes />
      <isActiveFunction>return JobType === "Recurring";</isActiveFunction>
      <isRequiredFunction>return true;</isRequiredFunction>
    </property>
    <property id="e5a5f7ad-80f9-471c-9e0f-835f8286ffab">
      <hint>The delayed job delay time frame</hint>
      <name>Delay Time Frame</name>
      <type>string</type>
      <controlType>select</controlType>
      <optionsSource>options</optionsSource>
      <placeholder />
      <defaultValue>Minutes</defaultValue>
      <valueOptions>
        <option>Milliseconds</option>
        <option>Seconds</option>
        <option>Minutes</option>
        <option>Hours</option>
        <option>Days</option>
        <option>Ticks</option>
      </valueOptions>
      <lookupTypes />
      <isActiveFunction>return JobType === "Delayed";</isActiveFunction>
    </property>
    <property id="8220c0e1-203d-43f5-9155-ae73a87bf324">
      <hint />
      <name>Delay Value</name>
      <type>string</type>
      <controlType>number</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder />
      <defaultValue>5</defaultValue>
      <valueOptions />
      <lookupTypes />
      <isActiveFunction>return JobType === "Delayed";</isActiveFunction>
    </property>
    <property id="0aa4b661-211b-40e2-a917-abfc4fef1b03">
      <hint>Only execute this job once the previous instance is completed.</hint>
      <name>Disallow Concurrent Execution</name>
      <type>string</type>
      <controlType>checkbox</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder />
      <defaultValue>false</defaultValue>
      <valueOptions />
      <lookupTypes />
      <isActiveFunction>return JobType === "Recurring";</isActiveFunction>
    </property>
    <property id="722cc50a-5dfd-4eb0-91c2-287ddd077d04">
      <hint>The amount of time (in seconds) before a concurrent job waiting execution is cancelled</hint>
      <name>Concurrent Execution Timeout</name>
      <type>string</type>
      <controlType>number</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder />
      <defaultValue>1</defaultValue>
      <valueOptions />
      <lookupTypes />
      <isActiveFunction>return JobType === "Recurring";</isActiveFunction>
    </property>
    <property id="7e719fad-42a3-4282-8328-e086d856dee7">
      <hint>The queue on which the job is executed. If no queue is supplied, the default queue is used.</hint>
      <name>Queue</name>
      <type>string</type>
      <controlType>select</controlType>
      <optionsSource>lookup-element</optionsSource>
      <placeholder>Default</placeholder>
      <defaultValue />
      <valueOptions />
      <lookupTypes>
        <type>18b0f37e-43da-4d94-b65a-8cff601339ac</type>
      </lookupTypes>
    </property>
  </properties>
</stereotypeDefinition>